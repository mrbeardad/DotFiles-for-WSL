{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "<darkGray,transparent>╭─</>",
          "trailing_diamond": "",
          "foreground": "white",
          "background": "cyan",
          "template": " {{ .Icon }} "
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "lightYellow",
          "background": "lightRed",
          "template": " 󰒃 "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "white",
          "background": "blue",
          "template": " {{ path .Path .Location }} ",
          "properties": {
            "style": "full",
            "home_icon": " ",
            "folder_icon": " ",
            "folder_separator_icon": " ",
            "mapped_locations": {
              "~/AppData/Roaming": "󱑠 ",
              "~/AppData/Local": " ",
              "~/AppData/Local/Temp": " "
            }
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "black",
          "background": "green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}lightYellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}lightRed{{ end }}",
            "{{ if gt .Ahead 0 }}lightMagenta{{ end }}",
            "{{ if gt .Behind 0 }}lightMagenta{{ end }}"
          ],
          "templates": [
            " {{ url .UpstreamIcon .UpstreamURL }}",
            " {{ .HEAD }}",
            "{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}",
            "{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}",
            "{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}",
            "{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}"
          ],
          "properties": {
            "fetch_upstream_icon": true,
            "fetch_status": true,
            "branch_icon": "",
            "branch_identical_icon": "≡",
            "branch_gone_icon": "󰦎",
            "branch_ahead_icon": "↑",
            "branch_behind_icon": "↓",
            "commit_icon": "",
            "no_commits_icon": "󰂕 ",
            "tag_icon": " ",
            "merge_icon": " ",
            "rebase_icon": " ",
            "cherry_pick_icon": " ",
            "revert_icon": " "
          }
        },
        {
          "type": "cmake",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#E8EAEE",
          "background": "#1E9748",
          "template": "  cmake {{ .Full }} "
        },
        {
          "type": "xmake",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#e0f2f1",
          "background": "#22a079",
          "template": "  xmake {{ .Full }} "
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#193549",
          "background": "#99908a",
          "template": "  {{ .Full }} "
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#7FD5EA",
          "template": "  {{ .Full }} "
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": "  {{ .Full }} "
        },
        {
          "type": "deno",
          "style": "plain",
          "foreground": "#3C82F6",
          "template": "  {{ .Full }} "
        },
        {
          "type": "dart",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#06A4CE",
          "template": "  {{ .Full }} "
        },
        {
          "type": "flutter",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#06A4CE",
          "template": "  {{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#100e23",
          "background": "#906cff",
          "template": "  {{ .Full }} "
        },
        {
          "type": "lua",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#000081",
          "template": "  {{ .Full }} "
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#000000",
          "background": "#00ffff",
          "template": "  {{ .Full }} "
        },
        {
          "type": "haskell",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#906cff",
          "background": "#100e23",
          "template": "  {{ .Full }}"
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": "  {{ .Full }}"
        },
        {
          "type": "perl",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": "  {{ .Full }}"
        },
        {
          "type": "php",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": "  {{ .Full }} "
        },
        {
          "type": "r",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "blue",
          "background": "lightWhite",
          "template": " R {{ .Full }} "
        },
        {
          "type": "ruby",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": "  {{ .Full }}"
        },
        {
          "type": "swift",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ffffff",
          "background": "#f6553c",
          "template": "  {{ .Full }} "
        },
        {
          "type": "unity",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#111111",
          "background": "#ffffff",
          "properties": {
            "http_timeout": 2000
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "lightWhite",
          "background": "darkGray",
          "leading_diamond": "<transparent,background></>",
          "trailing_diamond": "",
          "template": " 󰔚 {{ .FormattedMs }}",
          "properties": {
            "style": "austin",
            "threshold": 0
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "sysinfo",
          "style": "diamond",
          "foreground": "black",
          "background": "white",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,background></>",
          "template": " {{ round .PhysicalPercentUsed .Precision }}% "
        },
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,background></>",
          "foreground": "white",
          "background": "magenta",
          "templates": [
            "{{ if .Env.HTTP_PROXY }} 󰒍 {{ .Env.HTTP_PROXY }} {{ end }}",
            "{{ if .Env.HTTPS_PROXY }} 󰒍 {{ .Env.HTTPS_PROXY }} {{ end }}",
            "{{ if .Env.ALL_PROXY }} 󰒍 {{ .Env.ALL_PROXY }} {{ end }}"
          ],
          "templates_logic": "first_match"
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "white",
          "background": "cyan",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "  {{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ],
      "overflow": "break"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "templates": [
            "<darkGray>╰─</>",
            "{{ if .Env.PoshDefaultCursorShape }}{{ .Env.PoshDefaultCursorShape }}{{ end }}",
            " {{ if ne .Code 0 }}<lightRed>❯</><red>❯</>{{ else }}<lightBlue>❯</><lightCyan>❯</>{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "newline": true
    }
  ],
  "transient_prompt": {
    "foreground_templates": [
      "{{ if ne .Code 0 }}lightRed{{ else }}lightGreen{{ end }}"
    ],
    "template": " "
  }
}
