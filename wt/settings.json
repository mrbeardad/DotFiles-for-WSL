{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "keys": "win+`",
      "command": { "action": "globalSummon", "name": "_quake" }
    },
    {
      "keys": "alt+shift+v",
      "command": { "action": "splitPane", "split": "vertical" }
    },
    {
      "keys": "alt+shift+s",
      "command": { "action": "splitPane", "split": "horizontal" }
    },
    {
      "keys": "alt+shift+z",
      "command": "togglePaneZoom"
    },
    {
      "keys": "ctrl+shift+t",
      "command": { "action": "newTab", "commandline": "btop" }
    },
    {
      "keys": "ctrl+i",
      "command": {
        "action": "sendInput",
        "input": "\u001b[73;4u" // <A-I>
      }
    },
    {
      "keys": "ctrl+shift+j",
      "command": {
        "action": "sendInput",
        "input": "\u001b[74;5u"
      }
    },
    {
      "keys": "ctrl+shift+k",
      "command": {
        "action": "sendInput",
        "input": "\u001b[75;5u"
      }
    },
    {
      "keys": "ctrl+enter",
      "command": {
        "action": "sendInput",
        "input": "\u001b[13;5u"
      }
    },
    {
      "keys": "shift+enter",
      "command": {
        "action": "sendInput",
        "input": "\u001b[13;2u"
      }
    }
  ],
  "copyOnSelect": true,
  "multiLinePasteWarning": false,
  "tabSwitcherMode": "mru",
  "theme": "system",
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "profiles": {
    "defaults": {
      "colorScheme": {
        "dark": "Bluloco Dark",
        "light": "Bluloco Light"
      },
      "font": {
        "face": "NerdCodePro",
        "size": 10
      },
      // "useAtlasEngine": false, // Not yet mature
      "intenseTextStyle": "all",
      "padding": "0, 0, 0, 0",
      "scrollbarState": "hidden",
      "opacity": 50,
      "useAcrylic": true
    },
    "list": [
      {
        "name": "PowerShell",
        "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
        "source": "Windows.Terminal.PowershellCore"
      },
      {
        "name": "Cloud Virtual Machine",
        "guid": "{a453ae62-4e3d-5e58-b989-0a998ec441b8}",
        "commandline": "ssh -t -o ServerAliveInterval=60 ubuntu@43.134.204.157 sh -c 'tmux new-session -s Ubuntu || tmux attach -t Ubuntu'"
      },
      {
        "name": "Windows PowerShell",
        "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
        "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
      },
      {
        "name": "Command Prompt",
        "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
        "commandline": "%SystemRoot%\\System32\\cmd.exe"
      },
      {
